{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "enablePullDownRefresh":true, //开启下拉刷新
        "app-plus": { "titleNView": false}
      }
    },
    {
      "path": "pages/information/index",
      "style": {
        "navigationBarTitleText": "资讯"
      }
    },
    {
      "path": "pages/article/index",
      "style": {
        "navigationBarTitleText": "图文",
        "app-plus": { "titleNView": false}
      }
    },
    {
      "path": "pages/user/index",
      "style": {
        "navigationBarTitleText": "个人中心",
        "app-plus": { "titleNView": false}
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "晴天",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#409eff",
    "borderStyle": "black",
    "backgroundColor": "#f5f5f5",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/index.png",
        "selectedIconPath": "static/indexCur.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/information/index",
        "iconPath": "static/order.png",
        "selectedIconPath": "static/orderCur.png",
        "text": "资讯"
      },
      {
        "pagePath": "pages/article/index",
        "iconPath": "static/service.png",
        "selectedIconPath": "static/serviceCur.png",
        "text": "图文"
      },
      {
        "pagePath": "pages/user/index",
        "iconPath": "static/my.png",
        "selectedIconPath": "static/myCur.png",
        "text": "我的"
      }
    ],
    "midButton": {
      "width": "80px",
      "height": "50px",
      "text": "文字",
      "iconPath": "static/logo.png",
      "iconWidth": "24px",
      "backgroundImage": "static/logo.png"
    }
  }
}